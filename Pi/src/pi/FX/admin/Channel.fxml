<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="350.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pi.controller.ChannelController">
   <children>
      <TableView fx:id="channel_tableView" layoutX="330.0" layoutY="50.0" onMouseClicked="#availableChannelSelect" prefHeight="228.0" prefWidth="345.0" stylesheets="@../dashboardDesign.css">
         <columns>
            <TableColumn fx:id="column_nom" prefWidth="161.0" text="Nom Channel" />
            <TableColumn fx:id="column_evenement" prefWidth="180.0" text="Evenement" />
         </columns>
      </TableView>
      <Label layoutX="72.0" layoutY="24.0" prefHeight="21.0" prefWidth="101.0" text="CHANNEL" textFill="#66944c">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <TextField fx:id="txt_nom" layoutX="108.0" layoutY="73.0" />
      <Label layoutX="21.0" layoutY="78.0" prefHeight="21.0" prefWidth="38.0" text="Titre" />
      <Label layoutX="22.0" layoutY="129.0" text="Evenement" />
      <ComboBox fx:id="combo_evenement" layoutX="108.0" layoutY="124.0" prefHeight="31.0" prefWidth="187.0" />
      <Button fx:id="btn_update" layoutX="180.0" layoutY="216.0" mnemonicParsing="false" onAction="#updateOnClick" prefHeight="31.0" prefWidth="94.0" styleClass="update-btn" stylesheets="@../dashboardDesign.css" text="modifier" />
      <Button fx:id="btn_add" layoutX="66.0" layoutY="216.0" mnemonicParsing="false" onAction="#addOnClick" prefHeight="31.0" prefWidth="84.0" styleClass="add-btn" stylesheets="@../dashboardDesign.css" text="ajouter" />
      <Button fx:id="btn_clear" layoutX="194.0" layoutY="263.0" mnemonicParsing="false" onAction="#clearOnClick" prefHeight="31.0" prefWidth="101.0" styleClass="clear-btn" stylesheets="@../dashboardDesign.css" text="annuler" />
      <Button fx:id="btn_delete" layoutX="53.0" layoutY="263.0" mnemonicParsing="false" onAction="#deleteOnClick" prefHeight="31.0" prefWidth="94.0" styleClass="delete-btn" stylesheets="@../dashboardDesign.css" text="supprimer" />
   </children>
</AnchorPane>
